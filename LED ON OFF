//basic led control ON OFF//

#define led 32 //set led is port D32

void setup () {
  pinMode(led, OUTPUT); //set pin led use for output

}

void loop () {
  digitalWrite(led, HIGH); //LED ON
  delay(500); //delay 500 millisecond
  
  digitalWrite(led, LOW); //LED OFF
  delay(500); //delay 500 millisecond 
  
}
